# Airtable Configuration
AIRTABLE_API_KEY=your_api_key_here
AIRTABLE_BASE_ID=appXXXXXXXXXXXXXX
AIRTABLE_TABLE_ID=tblXXXXXXXXXXXXXX

# Redis Configuration
REDIS_URL=//localhost:6379

# Cache TTL in seconds (5 minutes)
CACHE_TTL=300

# API Configuration
API_TITLE="Airtable Orders Dashboard API"
API_VERSION="1.0.0"
API_DESCRIPTION="Backend API for managing and visualizing Airtable orders"

# CORS
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# Rate Limiting (requests per minute)
RATE_LIMIT_ORDERS_LIST=100/minute
RATE_LIMIT_ORDERS_SUMMARY=60/minute
RATE_LIMIT_ORDERS_GET=100/minute
RATE_LIMIT_ORDERS_UPDATE=30/minute
RATE_LIMIT_ORDERS_SYNC=5/minute
